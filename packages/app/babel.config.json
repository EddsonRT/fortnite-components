{
    "presets": [
        ["@babel/preset-env", { "targets": { "node": "current" } }],
        [
            "next/babel",
            {
                "preset-react": {
                    "runtime": "automatic",
                    "importSource": "@emotion/react"
                }
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript",
        "@emotion/babel-preset-css-prop"
    ],
    "plugins": [
        "@emotion/babel-plugin",
        "babel-plugin-twin",
        "babel-plugin-macros",
        [
            "transform-imports",
            {
                "@web/components": {
                    "transform": "@web/components/${member}",
                    "preventFullImport": true
                }
            }
        ]
    ]
}
